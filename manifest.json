{
    "name": "Whatsapp TLDR Bot",
    "version": "1.0",
    "description": "Base Level Extension",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extensions.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
    "permissions": [
      "storage"
    ]
}
